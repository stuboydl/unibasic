{
   "name": "unibasic",
   "publisher": "stuboydl",
   "displayName": "UniBasic",
   "description": "Rocket UniVerse Basic. VS Code Syntax extension",
   "version": "0.0.6",
   "license": "MIT",
   "homepage": "https://github.com/stuboydl/unibasic/blob/master/README.md",
   "repository": {
      "type": "git",
      "url": "https://github.com/stuboydl/unibasic"
   },
   "bugs": {
      "url": "https://github.com/stuboydl/unibasic/issues"
   },
   "galleryBanner": {
      "color": "#0000FF",
      "theme": "dark"
   },
   "engines": {
      "vscode": "^1.30.0"
   },
   "categories": [
      "Programming Languages"
   ],
   "activationEvents": [],
   "main": "./out/extension.js",
   "contributes": {
      "languages": [
         {
            "id": "unibasic",
            "aliases": [
               "UniBasic",
               "unibasic"
            ],
            "extensions": [
               ""
            ],
            "configuration": "./language-configuration.json"
         }
      ],
      "snippets": [
         {
            "language": "unibasic",
            "path": "./snippets/unibasic.json"
         }
      ],
      "grammars": [
         {
            "language": "unibasic",
            "scopeName": "source.unibasic",
            "path": "./syntaxes/unibasic.tmLanguage.json"
         }
      ],
      "commands": []
   },
   "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -p ./",
      "watch": "tsc -watch -p ./",
      "postinstall": "node ./node_modules/vscode/bin/install",
      "test": "npm run compile && node ./node_modules/vscode/bin/test"
   },
   "devDependencies": {
      "typescript": "^3.5.1",
      "vscode": "^1.1.34",
      "tslint": "^5.17.0",
      "@types/node": "^12.0.5",
      "@types/mocha": "^5.2.7"
   }
}
